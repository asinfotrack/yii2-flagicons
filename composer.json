{
	"name": "asinfotrack/yii2-flagicons",
	"type": "yii2-extension",
	
	"description": "Yii2-flagicons is a Yii2-wrapper around the flag-icon-css-project. With this extension you can easily use vector-based country flags in your Yii2-project.",
	"keywords": ["yii2", "flag", "icons", "vector", "widget", "bundle"],
	
	"license": "MIT",
	"authors": [
		{
			"name": "Pascal Mueller",
			"email": "pascal.mueller@asinfotrack.ch",
			"homepage": "http://www.asinfotrack.ch"
		}
	],
	
	"minimum-stability": "stable",
	"support": {
		"source": "https://github.com/asinfotrack/yii2-flagicons",
		"issues": "https://github.com/asinfotrack/yii2-flagicons/issues"
	},
	
	"require": {
		"php": ">=5.4.0",
		
		"yiisoft/yii2": "2.0.*",
		"components/flag-icon-css": "2.6.0"
	},
	
	"autoload": {
		"psr-4": {
			"asinfotrack\\yii2\\flagicons\\": ""
		}
	}
}
